    "keywords": ["csv", "parser", "product", "csv parser", "product parser", "parsecsv"],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Yaroslav P",
            "homepage": "https://github.com/decss"
        }
    ],
    "autoload": {
        "psr-4": {
            "ItemParser\\": "src"
        }
    },
    "require": {
        "php": ">=5.5",
        "ext-mbstring": "*",
        "ext-json": "*",
        "ext-iconv": "*",
        "parsecsv/php-parsecsv": "^1.2"
    },
    "require-dev": {
        "symfony/var-dumper": "^3.4"
